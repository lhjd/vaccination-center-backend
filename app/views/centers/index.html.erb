


<div class="row">
  <div class="col-sm-6">
    <h1>Centers</h1>
  </div>

  <div class="col-sm-6 text-end">
  <%= link_to new_center_path, class: 'btn btn-primary' do %>
    Add New Center
  <% end %>
  </div>
</div>

<div class="table-responsive">
  <table class="table table-striped table-bordered table-hover">
    <thead>
      <tr>
    
        <th>Name</th>
    

            
      </tr>
    </thead>

    <tbody>
      <% @centers.each do |center| %>
        <%= content_tag :tr, id: dom_id(center), class: dom_class(center) do %>
          
            <td><%= link_to center.name, center %></td>
          

          
          
        <% end %>
      <% end %>
    </tbody>
  </table>
</div>
